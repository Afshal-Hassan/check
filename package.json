{
  "name": "look-a-like-match",
  "version": "1.1.0",
  "private": true,
  "description": "Enterprise-grade dating platform API built with Node.js, Express, TypeScript, and TypeORM",
  "main": "src/server.ts",
  "license": "UNLICENSED",
  "author": {
    "name": "Launchbox Global Team",
    "email": "afshal.hassan@launchboxglobal.co"
  },
  "scripts": {
    "dev": "nodemon",
    "start": "node dist/server.js",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typeorm": "typeorm-ts-node-commonjs",
    "migrate:create": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create",
    "migrate:generate": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:generate -d src/config/data-source.ts",
    "migrate:run": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run -d src/config/data-source.ts",
    "migrate:revert": "npx ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert -d src/config/data-source.ts",
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production' && !process.env.CI) { require('husky').install() }\""
  },
  "_moduleAliases": {
    "@": "dist"
  },
  "dependencies": {
    "@aws-sdk/client-rekognition": "^3.975.0",
    "@aws-sdk/client-s3": "^3.971.0",
    "@aws-sdk/client-translate": "^3.972.0",
    "@aws-sdk/s3-request-presigner": "^3.971.0",
    "@vitalets/google-translate-api": "^9.2.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "jsonwebtoken": "^9.0.3",
    "module-alias": "^2.2.3",
    "multer": "^2.0.2",
    "multer-s3": "^3.0.1",
    "nodemailer": "^7.0.12",
    "pg": "^8.16.3",
    "redis": "^5.10.0",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.28"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^25.0.8",
    "@types/nodemailer": "^7.0.5",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "nodemon": "^3.1.11",
    "prettier": "^3.7.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LaunchBoxGlobal/LAL-backend.git"
  },
  "keywords": [
    "dating-app",
    "matchmaking",
    "social-network",
    "real-time-chat",
    "geolocation",
    "nodejs",
    "typescript",
    "express",
    "typeorm",
    "postgresql",
    "redis",
    "socket.io"
  ],
  "bugs": {
    "url": "https://github.com/LaunchBoxGlobal/LAL-Backend/issues"
  }
}
